#################################################################
#    Classes
#################################################################

###  http://example.org/festividades#Colectivo
:Colectivo rdf:type owl:Class ;
           rdfs:subClassOf :Participante ;
           rdfs:comment "Participante colectivo (grupo, comunidad)."@es ;
           rdfs:label "Colectivo"@es .


###  http://example.org/festividades#Danza
:Danza rdf:type owl:Class ;
       rdfs:subClassOf :PracticaCultural ;
       rdfs:comment "Práctica ritual relativamente estable, compuesta por un repertorio definido de movimientos, vestimenta, música y roles. Funciona como una forma de participación colectiva dentro de una festividad, articulando a personas, comunidades y territorios a través de la performance ritual. En el contexto de Qoyllur Rit’i, una danza se entiende como una tradición o comparsa asociada a una nación o comunidad específica, que se ejecuta en eventos rituales concretos: misas, ingresos ceremoniales, ascensos y descensos rituales, o caminatas colectivas."@es ;
       rdfs:label "Danza"@es .


###  http://example.org/festividades#Danzante
:Danzante rdf:type owl:Class ;
          rdfs:subClassOf :Individuo ;
          rdfs:comment "Participante que ejecuta danzas rituales."@es ;
          rdfs:label "Danzante"@es .


###  http://example.org/festividades#EventoRitual
:EventoRitual rdf:type owl:Class ;
              rdfs:label "EventoRitual" .


###  http://example.org/festividades#Festividad
:Festividad rdf:type owl:Class ;
            rdfs:comment "Celebración ritual de carácter anual o periódico." ;
            rdfs:label "Festividad" .


###  http://example.org/festividades#Individuo
:Individuo rdf:type owl:Class ;
           rdfs:subClassOf :Participante ;
           rdfs:comment "Participante individual (persona)."@es ;
           rdfs:label "Individuo"@es .


###  http://example.org/festividades#Lugar
:Lugar rdf:type owl:Class ;
       rdfs:label "Lugar" .


###  http://example.org/festividades#Nacion
:Nacion rdf:type owl:Class ;
        rdfs:subClassOf :Colectivo ;
        rdfs:label "Nacion" .


###  http://example.org/festividades#ObjetoRitual
:ObjetoRitual rdf:type owl:Class ;
              rdfs:comment "Artefacto material utilizado en ceremonias con significado simbólico."@es ;
              rdfs:label "Objeto Ritual"@es .


###  http://example.org/festividades#Participante
:Participante rdf:type owl:Class ;
              rdfs:comment "Actor individual o colectivo que toma parte en la festividad del Señor de Qoyllu Rit'i."@es ;
              rdfs:label "Participante"@es .


###  http://example.org/festividades#Peregrino
:Peregrino rdf:type owl:Class ;
           rdfs:subClassOf :Individuo ;
           rdfs:comment "Participante que realiza la peregrinación sin un rol ritual específico."@es ;
           rdfs:label "Peregrino"@es .


###  http://example.org/festividades#PracticaCultural
:PracticaCultural rdf:type owl:Class ;
                  rdfs:comment "Manifestación cultural de carácter ritual, artístico o tradicional dentro del contexto de la Festividad del Señor de Qoyllu Rit'i."@es ;
                  rdfs:label "Práctica Cultural"@es .


###  http://example.org/festividades#RecursoMultimedia
:RecursoMultimedia rdf:type owl:Class ;
                   rdfs:label "RecursoMultimedia" .


###  http://example.org/festividades#Ukumari
:Ukumari rdf:type owl:Class ;
         rdfs:subClassOf :Danzante ;
         rdfs:comment "Danzante ritual asociado al oso andino, conocido también como ukuku. En Qoyllur Rit’i, el ukumari constituye una identidad ritual integral que articula práctica de danza, dimensión espiritual y funciones sociales específicas dentro del orden ceremonial."@es ;
         rdfs:label "Ukumari (Ukuku)" .


###  http://example.org/festividades#Vestimenta
:Vestimenta rdf:type owl:Class ;
            rdfs:subClassOf :ObjetoRitual ;
            rdfs:comment "Indumentaria ceremonial."@es ;
            rdfs:label "Vestimenta"@es .


